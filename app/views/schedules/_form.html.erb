<%= form_with(model: schedule) do |form| %>
  <% if schedule.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(schedule.errors.count, "error") %> prohibited this schedule from being saved:</h2>

      <ul>
        <% schedule.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :json_schedule, style: "display: block" %>
    <%= form.text_field :json_schedule %>
  </div>

  <div>
    <%= form.label :raw_text_schedule, style: "display: block" %>
    <%= form.text_area :raw_text_schedule %>
  </div>

  <div>
    <%= form.label :project_id, style: "display: block" %>
    <%= form.number_field :project_id %>
  </div>

  <div>
    <%= form.label :sprint_id, style: "display: block" %>
    <%= form.number_field :sprint_id %>
  </div>

  <div>
    <%= form.label :user_id, style: "display: block" %>
    <%= form.number_field :user_id %>
  </div>

  <div>
    <%= form.label :manager_id, style: "display: block" %>
    <%= form.number_field :manager_id %>
  </div>

  <div>
    <%= form.label :is_approved, style: "display: block" %>
    <%= form.check_box :is_approved %>
  </div>

  <div>
    <%= form.label :approved_at, style: "display: block" %>
    <%= form.number_field :approved_at %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
